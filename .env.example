# RabbitMQ bağlantısı
RABBITMQ_URL=amqp://guest:guest@rabbitmq:5672/

# Kuyruk adı
METRICS_QUEUE=metrics

# Bu ajanı benzersiz tanımlayan isim (hostname yerine geçer)
AGENT_ID=server-01

# Gönderim aralığı (saniye)
INTERVAL=30

# İsteğe bağlı eşik değerleri
MAX_CPU_THRESHOLD=95.0
MAX_MEMORY_THRESHOLD=95.0
MAX_DISK_THRESHOLD=95.0
